{
  "assistant": {
    "default_model": {
      "provider": "ollama",
      "model": "deepseek-r1:1.5b"
    },
    "version": "2"
  },
  "ui_font_size": 12,
  "buffer_font_size": 12,
  "theme": "One Dark",
  "ui_font_family": "Hack Nerd Font Mono",
  "vim_mode": true,
  "cursor_blink": false,
  "cursor_shape": "underline",
  "scrollbar": { "show": "never" },
  "inlay_hints": { "enabled": true },
  "collaboration_panel": { "button": false },
  "chat_panel": { "button": "never" },
  "notification_panel": { "button": false },
  "tab_size": 2,
  "auto_update": false,
  "terminal": { "cursor_shape": "underline", "button": false },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  }
}
